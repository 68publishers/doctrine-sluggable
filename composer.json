{
	"name": "68publishers/doctrine-sluggable",
	"description": "Doctrine Sluggable extension",
	"keywords": ["68publishers", "remote-access"],
	"license": "MIT",
	"authors": [
		{
			"name": "Tomáš Glawaty",
			"email": "tomasglawaty@icloud.com"
		}
	],
	"require": {
		"php": "^7.1 || ^8.1",
		"doctrine/orm": "^2.7",
		"behat/transliterator": "^1.2"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"friendsofphp/php-cs-fixer": "^2.19",
		"nette/tester": "^1.7",
		"mockery/mockery": "^1.2",
		"nette/di": "^2.4"
	},
	"suggest": {
		"nette/di": "For integration into Nette Framework"
	},
	"autoload": {
		"psr-4": {
			"SixtyEightPublishers\\DoctrineSluggable\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SixtyEightPublishers\\DoctrineSluggable\\Tests\\": "tests/"
		}
	}
}
